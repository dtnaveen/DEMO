            })
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "text-center",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-2xl font-bold text-gray-900",
              children: formatTime(recordingTime)
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-sm text-gray-600",
              children: "Recording..."
            })]
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
          onClick: stopRecording,
          className: "w-full flex items-center justify-center gap-3 px-6 py-4 bg-red-500 text-white rounded-xl hover:bg-red-600 transition-all",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_outline.StopIcon, {
            className: "w-6 h-6"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
            className: "font-bold",
            children: "Stop Recording"
          })]
        })]
      }))
    })) :
    /* istanbul ignore next */
    (/*#__PURE__*/cov_nudncdk8i().b[16][1]++, (0, _jsxruntime.jsxs)("div", {
      className: "space-y-4",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex items-center gap-4 p-4 bg-gray-50 rounded-lg",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: togglePlayback,
          className: "w-12 h-12 bg-primary-500 text-white rounded-full flex items-center justify-center hover:bg-primary-600 transition-all",
          children: isPlaying ?
          /* istanbul ignore next */
          (/*#__PURE__*/cov_nudncdk8i().b[18][0]++, (0, _jsxruntime.jsx)(_outline.PauseIcon, {
            className: "w-6 h-6"
          })) :
          /* istanbul ignore next */
          (/*#__PURE__*/cov_nudncdk8i().b[18][1]++, (0, _jsxruntime.jsx)(_outline.PlayIcon, {
            className: "w-6 h-6 ml-1"
          }))
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex-1",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "h-2 bg-gray-200 rounded-full overflow-hidden",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "h-full bg-primary-500"
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
            className: "text-sm text-gray-600 mt-2",
            children: audioRef.current ?
            /* istanbul ignore next */
            (cov_nudncdk8i().b[19][0]++, formatTime(Math.floor(
            /* istanbul ignore next */
            (cov_nudncdk8i().b[20][0]++, audioRef.current.duration) ||
            /* istanbul ignore next */
            (cov_nudncdk8i().b[20][1]++, 0)))) :
            /* istanbul ignore next */
            (cov_nudncdk8i().b[19][1]++, '0:00')
          })]
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("audio", {
        ref: audioRef,
        src: audioUrl,
        onEnded: () => {
          /* istanbul ignore next */
          cov_nudncdk8i().f[16]++;
          cov_nudncdk8i().s[79]++;
          return setIsPlaying(false);
        },
        className: "hidden"
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex gap-2",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: handleSend,
          className: "flex-1 px-4 py-2 bg-primary-500 text-white rounded-lg hover:bg-primary-600 transition-all font-medium",
          children: "Send Voice Message"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: handleCancel,
          className: "px-4 py-2 bg-gray-200 text-gray-700 rounded-lg hover:bg-gray-300 transition-all font-medium",
          children: "Cancel"
        })]
      })]
    }))
  });
}
