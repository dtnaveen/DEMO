              cov_1alz65w6h9().f[14]++;
              cov_1alz65w6h9().s[44]++;
              return reorderPhoto(index, index - 1);
            },
            className: "absolute -left-2 top-1/2 -translate-y-1/2 bg-primary-500 text-white rounded-full p-1 opacity-0 group-hover:opacity-100 transition-opacity",
            children: "←"
          })),
          /* istanbul ignore next */
          (cov_1alz65w6h9().b[9][0]++, index < photos.length - 1) &&
          /* istanbul ignore next */
          (cov_1alz65w6h9().b[9][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_1alz65w6h9().f[15]++;
              cov_1alz65w6h9().s[45]++;
              return reorderPhoto(index, index + 1);
            },
            className: "absolute -right-2 top-1/2 -translate-y-1/2 bg-primary-500 text-white rounded-full p-1 opacity-0 group-hover:opacity-100 transition-opacity",
            children: "→"
          }))]
        }, photo.id);
      }),
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[10][0]++, photos.length < maxPhotos) &&
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[10][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        onDrop: handleDrop,
        onDragOver: handleDragOver,
        onDragLeave: handleDragLeave,
        onClick: () => {
          /* istanbul ignore next */
          cov_1alz65w6h9().f[16]++;
          cov_1alz65w6h9().s[46]++;
          return fileInputRef.current?.click();
        },
        className: `aspect-square rounded-xl border-2 border-dashed flex flex-col items-center justify-center cursor-pointer transition-all ${isDragging ?
        /* istanbul ignore next */
        (cov_1alz65w6h9().b[11][0]++, 'border-primary-500 bg-primary-50') :
        /* istanbul ignore next */
        (cov_1alz65w6h9().b[11][1]++, 'border-gray-300 hover:border-primary-400 hover:bg-gray-50')}`,
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_outline.PhotoIcon, {
          className: "w-12 h-12 text-gray-400 mb-2"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
          className: "text-sm text-gray-600 font-medium",
          children: "Add Photo"
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("p", {
          className: "text-xs text-gray-400 mt-1",
          children: [photos.length, "/", maxPhotos]
        })]
      }))]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
      ref: fileInputRef,
      type: "file",
      multiple: true,
      accept: "image/*",
      onChange: handleFileInput,
      className: "hidden"
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
      className: "text-sm text-gray-600",
      children: photos.length < minPhotos ?
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[12][0]++, `Add at least ${minPhotos} photo${minPhotos > 1 ?
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[13][0]++, 's') :
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[13][1]++, '')} to continue`) :
      /* istanbul ignore next */
      (cov_1alz65w6h9().b[12][1]++, `You can add up to ${maxPhotos} photos. Drag to reorder.`)
    })]
  });
}
