                                                                    ]
                                                                }),
                                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                    children: [
                                                                        post.views,
                                                                        " views"
                                                                    ]
                                                                }),
                                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                    className: "flex items-center gap-1",
                                                                    children: [
                                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ClockIcon, {
                                                                            className: "w-4 h-4"
                                                                        }),
                                                                        post.lastActivity
                                                                    ]
                                                                }),
                                                                post.likes > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                    className: "text-primary-600",
                                                                    children: [
                                                                        "â¤ï¸ ",
                                                                        post.likes
                                                                    ]
                                                                })
                                                            ]
                                                        })
                                                    ]
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                                    variant: "outline",
                                                    size: "sm",
                                                    onClick: (e)=>{
                                                        e.stopPropagation();
                                                        handleViewPost(post.id);
                                                    },
                                                    className: "w-full sm:w-auto",
                                                    children: "View"
                                                })
                                            ]
                                        })
                                    }, post.id))
                            }),
                            totalPostPages > 1 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "mt-8 flex flex-wrap items-center justify-center gap-2",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                        onClick: ()=>setCurrentPage((prev)=>Math.max(1, prev - 1)),
                                        disabled: currentPage === 1,
                                        variant: "outline",
                                        size: "sm",
                                        children: "Previous"
                                    }),
                                    Array.from({
                                        length: totalPostPages
                                    }, (_, i)=>i + 1).map((page)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                            onClick: ()=>setCurrentPage(page),
                                            variant: currentPage === page ? 'primary' : 'outline',
                                            size: "sm",
                                            className: "min-w-[40px]",
                                            children: page
                                        }, page)),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.default, {
                                        onClick: ()=>setCurrentPage((prev)=>Math.min(totalPostPages, prev + 1)),
                                        disabled: currentPage === totalPostPages,
                                        variant: "outline",
                                        size: "sm",
                                        children: "Next"
                                    })
                                ]
                            })
                        ]
                    })
                ]
            })
        })
    });
}

