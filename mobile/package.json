{
  "name": "vibematch-mobile",
  "version": "1.0.0",
  "description": "VibeMatch Native Mobile Apps",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && gradlew assembleRelease",
    "build:android:bundle": "cd android && gradlew bundleRelease",
    "build:ios": "echo 'iOS builds require Xcode on Mac. Open ios/VibeMatchMobile.xcworkspace in Xcode and Archive.'",
    "clean:android": "cd android && gradlew clean",
    "clean:ios": "cd ios && rm -rf build Pods Podfile.lock && pod install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-camera": "^4.2.1",
    "react-native-video": "^5.2.1",
    "react-native-webrtc": "^111.0.0",
    "react-native-audio-recorder-player": "^3.6.0",
    "react-native-permissions": "^3.10.1",
    "react-native-geolocation-service": "^5.3.1",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-push-notification": "^8.1.1",
    "react-native-iap": "^12.9.0",
    "react-native-biometrics": "^3.0.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
