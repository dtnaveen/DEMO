    "test:ci": "jest --ci --coverage --maxWorkers=1 --forceExit",
    "test:unit": "jest --testPathPattern=__tests__/lib --maxWorkers=1 --forceExit --testTimeout=2000",
    "test:components": "jest --testPathPattern=__tests__/components --maxWorkers=1 --forceExit --testTimeout=2000",
    "test:integration": "jest --testPathPattern=__tests__/integration --maxWorkers=1 --forceExit --testTimeout=2000",
    "test:fast": "jest --maxWorkers=1 --testTimeout=1000 --forceExit --bail"
